/* === Кнопки === */
QPushButton {
    background-color: #00cc88;
    color: white;
    border: none;
    border-radius: 8px;
    padding: 8px 18px;
    font-family: &quot;Stencil&quot;;
    font-weight: bold;
    text-transform: uppercase;
    letter-spacing: 1px;
}

QPushButton:hover {
    background-color: #00b377;
}

