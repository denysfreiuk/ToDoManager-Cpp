}

QPushButton:disabled {
    background-color: #cccccc;
    color: #777777;
}

/* === Чекбокс (Показати пароль) === */
QCheckBox {
    color: #303030;
    spacing: 6px;
}

QCheckBox::indicator {
    width: 16px;
    height: 16px;
    border: 2px solid #888888;
    border-radius: 4px;
    background-color: white;
}

QCheckBox::indicator:checked {
    background-color: #00cc88;
    border: 2px solid #00cc88;
}

QCheckBox:hover {
    color: #00aa66;
}

/* === Скролбар (на випадок, якщо є поле з прокруткою) === */
QScrollBar:vertical {
    border: none;
    background: #e0e0e0;
    width: 8px;
    margin: 0;
    border-radius: 4px;
}

QScrollBar::handle:vertical {
    background: #00cc88;
    border-radius: 4px;
    min-height: 20px;
}

QScrollBar::handle:vertical:hover {
    background: #00aa66;
}

QScrollBar::add-line:vertical,
QScrollBar::sub-line:vertical {
    height: 0;
}
